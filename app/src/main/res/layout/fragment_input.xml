<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="0sp"
    android:layout_marginStart="0sp"
    android:layout_marginLeft="0sp"
    android:layout_marginTop="0sp"
    android:layout_marginEnd="0sp"
    android:layout_marginRight="0sp"
    android:layout_marginBottom="2sp"
    tools:context=".InputFragment">

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="#D9D9D9"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView2"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="  시간표 설정"
                android:textColor="#FFFFFF"
                android:textSize="24sp" />

            <Space
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1" />

            <Space
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginBottom="1sp"
            android:layout_weight="1"
            android:background="#FFFFFF"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView13"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginBottom="2sp"
                android:layout_weight="1"
                android:backgroundTint="#FFFFFF"
                android:gravity="center"
                android:text="요일" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginBottom="2sp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="종료시간" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginBottom="0sp"
                android:layout_weight="1"
                android:backgroundTint="#FFFFFF"
                android:gravity="center"
                android:text="알림" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="#FFFFFF"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView14"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="0sp"
                android:layout_marginStart="0sp"
                android:layout_marginLeft="0sp"
                android:layout_marginTop="0sp"
                android:layout_marginEnd="0sp"
                android:layout_marginRight="0sp"
                android:layout_marginBottom="0sp"
                android:layout_weight="1"
                android:backgroundTint="#FFFFFF"
                android:gravity="center"
                android:text="Mon" />

            <Button
                android:id="@+id/btn_mon"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="0sp"
                android:layout_marginStart="0sp"
                android:layout_marginLeft="0sp"
                android:layout_marginTop="0sp"
                android:layout_marginEnd="0sp"
                android:layout_marginRight="0sp"
                android:layout_weight="1"
                android:backgroundTint="#D9D9D9"
                android:text="시간 입력" />

            <CheckBox
                android:id="@+id/chk_mon"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="0sp"
                android:layout_marginStart="0sp"
                android:layout_marginLeft="0sp"
                android:layout_marginTop="0sp"
                android:layout_marginEnd="0sp"
                android:layout_marginRight="0sp"
                android:layout_weight="1"
                android:backgroundTint="#FFFFFF"
                android:gravity="center" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="#FFFFFF"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView15"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="0sp"
                android:layout_marginStart="0sp"
                android:layout_marginLeft="0sp"
                android:layout_marginTop="0sp"
                android:layout_marginEnd="0sp"
                android:layout_marginRight="0sp"
                android:layout_marginBottom="0sp"
                android:layout_weight="1"
                android:backgroundTint="#FFFFFF"
                android:gravity="center"
                android:text="Tue" />

            <Button
                android:id="@+id/btn_tue"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="0sp"
                android:layout_marginStart="0sp"
                android:layout_marginLeft="0sp"
                android:layout_marginTop="0sp"
                android:layout_marginEnd="0sp"
                android:layout_marginRight="0sp"
                android:layout_weight="1"
                android:backgroundTint="#D9D9D9"
                android:text="시간 입력" />

            <CheckBox
                android:id="@+id/chk_tue"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="0sp"
                android:layout_marginStart="0sp"
                android:layout_marginLeft="0sp"
                android:layout_marginTop="0sp"
                android:layout_marginEnd="0sp"
                android:layout_marginRight="0sp"
                android:layout_weight="1"
                android:backgroundTint="#FFFFFF" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="#FFFFFF"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView16"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="0sp"
                android:layout_marginStart="0sp"
                android:layout_marginLeft="0sp"
                android:layout_marginTop="0sp"
                android:layout_marginEnd="0sp"
                android:layout_marginRight="0sp"
                android:layout_marginBottom="0sp"
                android:layout_weight="1"
                android:backgroundTint="#FFFFFF"
                android:gravity="center"
                android:text="Wed" />

            <Button
                android:id="@+id/btn_wen"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="0sp"
                android:layout_marginStart="0sp"
                android:layout_marginLeft="0sp"
                android:layout_marginTop="0sp"
                android:layout_marginEnd="0sp"
                android:layout_marginRight="0sp"
                android:layout_weight="1"
                android:backgroundTint="#D9D9D9"
                android:text="시간 입력" />

            <CheckBox
                android:id="@+id/chk_wen"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="0sp"
                android:layout_marginStart="0sp"
                android:layout_marginLeft="0sp"
                android:layout_marginTop="0sp"
                android:layout_marginEnd="0sp"
                android:layout_marginRight="0sp"
                android:layout_weight="1"
                android:backgroundTint="#FFFFFF" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="#FFFFFF"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView17"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="0sp"
                android:layout_marginStart="0sp"
                android:layout_marginLeft="0sp"
                android:layout_marginTop="0sp"
                android:layout_marginEnd="0sp"
                android:layout_marginRight="0sp"
                android:layout_marginBottom="0sp"
                android:layout_weight="1"
                android:backgroundTint="#FFFFFF"
                android:gravity="center"
                android:text="Thu" />

            <Button
                android:id="@+id/btn_tur"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="0sp"
                android:layout_marginStart="0sp"
                android:layout_marginLeft="0sp"
                android:layout_marginTop="0sp"
                android:layout_marginEnd="0sp"
                android:layout_marginRight="0sp"
                android:layout_weight="1"
                android:backgroundTint="#D9D9D9"
                android:text="시간 입력" />

            <CheckBox
                android:id="@+id/chk_tur"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="0sp"
                android:layout_marginStart="0sp"
                android:layout_marginLeft="0sp"
                android:layout_marginTop="0sp"
                android:layout_marginEnd="0sp"
                android:layout_marginRight="0sp"
                android:layout_weight="1"
                android:backgroundTint="#FFFFFF" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="#FFFFFF"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView18"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="0sp"
                android:layout_marginStart="0sp"
                android:layout_marginLeft="0sp"
                android:layout_marginTop="0sp"
                android:layout_marginEnd="0sp"
                android:layout_marginRight="0sp"
                android:layout_marginBottom="0sp"
                android:layout_weight="1"
                android:backgroundTint="#FFFFFF"
                android:gravity="center"
                android:text="Fri" />

            <Button
                android:id="@+id/btn_fri"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="0sp"
                android:layout_marginStart="0sp"
                android:layout_marginLeft="0sp"
                android:layout_marginTop="0sp"
                android:layout_marginEnd="0sp"
                android:layout_marginRight="0sp"
                android:layout_weight="1"
                android:backgroundTint="#D9D9D9"
                android:text="시간 입력" />

            <CheckBox
                android:id="@+id/chk_fri"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="0sp"
                android:layout_marginStart="0sp"
                android:layout_marginLeft="0sp"
                android:layout_marginTop="0sp"
                android:layout_marginEnd="0sp"
                android:layout_marginRight="0sp"
                android:layout_weight="1"
                android:backgroundTint="#FFFFFF" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="#FFFFFF"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView19"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="0sp"
                android:layout_marginStart="0sp"
                android:layout_marginLeft="0sp"
                android:layout_marginTop="0sp"
                android:layout_marginEnd="0sp"
                android:layout_marginRight="0sp"
                android:layout_marginBottom="0sp"
                android:layout_weight="1"
                android:backgroundTint="#FFFFFF"
                android:gravity="center"
                android:text="Sat" />

            <Button
                android:id="@+id/btn_sat"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="0sp"
                android:layout_marginStart="0sp"
                android:layout_marginLeft="0sp"
                android:layout_marginTop="0sp"
                android:layout_marginEnd="0sp"
                android:layout_marginRight="0sp"
                android:layout_weight="1"
                android:backgroundTint="#D9D9D9"
                android:text="시간 입력" />

            <CheckBox
                android:id="@+id/chk_sat"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="0sp"
                android:layout_marginStart="0sp"
                android:layout_marginLeft="0sp"
                android:layout_marginTop="0sp"
                android:layout_marginEnd="0sp"
                android:layout_marginRight="0sp"
                android:layout_weight="1"
                android:backgroundTint="#FFFFFF" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="#FFFFFF"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView20"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="0sp"
                android:layout_marginStart="0sp"
                android:layout_marginLeft="0sp"
                android:layout_marginTop="0sp"
                android:layout_marginEnd="0sp"
                android:layout_marginRight="0sp"
                android:layout_marginBottom="0sp"
                android:layout_weight="1"
                android:backgroundTint="#FFFFFF"
                android:gravity="center"
                android:text="Sun" />

            <Button
                android:id="@+id/btn_sun"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:backgroundTint="#D9D9D9"
                android:text="시간 입력" />

            <CheckBox
                android:id="@+id/chk_sun"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="0sp"
                android:layout_marginStart="0sp"
                android:layout_marginLeft="0sp"
                android:layout_marginTop="0sp"
                android:layout_marginEnd="0sp"
                android:layout_marginRight="0sp"
                android:layout_weight="1"
                android:backgroundTint="#FFFFFF" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="열차 탑승 방면"
                android:textSize="18sp" />

            <RadioGroup
                android:id="@+id/radioGroup_way"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">


                <RadioButton
                    android:id="@+id/radioButton_wayS"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="수색방면" />

                <RadioButton
                    android:id="@+id/radioButton_wayG"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="강매방면" />
            </RadioGroup>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="#7AB087"
            android:backgroundTint="#7AB097"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView100"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#7AB097"
                android:backgroundTint="#7AB097"
                android:gravity="center"
                android:text="수색방면"
                android:textColor="#FFFFFF"
                android:textSize="17sp" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="0dp"
                android:layout_height="20dp"
                android:layout_gravity="center"
                android:layout_marginStart="5sp"
                android:layout_marginTop="30sp"
                android:layout_marginRight="5sp"
                android:layout_marginBottom="30sp"
                android:layout_weight="0.3"
                android:background="#7AB097"
                android:backgroundTint="#7AB097"
                android:src="@android:drawable/button_onoff_indicator_off" />

            <TextView
                android:id="@+id/textView101"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#7AB097"
                android:backgroundTint="#7AB097"
                android:gravity="center"
                android:text="강매방면"
                android:textColor="#FFFFFF"
                android:textSize="17sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/text_wayFirst"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="청량리행"
                android:textColor="#000000"
                android:textSize="17sp" />

            <TextView
                android:id="@+id/text_timeFirst"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="5역전"
                android:textColor="#FF9900"
                android:textSize="17sp" />

            <TextView
                android:id="@+id/text_wayThird"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="문산행"
                android:textColor="#000000"
                android:textSize="17sp" />

            <TextView
                android:id="@+id/text_timeThird"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="00 : 00"
                android:textColor="#FF9900"
                android:textSize="17sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/text_waySecond"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="청량리행"
                android:textColor="#000000"
                android:textSize="17sp" />

            <TextView
                android:id="@+id/text_timeSecond"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="[급] 12역전"
                android:textColor="#FF9900"
                android:textSize="17sp" />

            <TextView
                android:id="@+id/text_wayFourth"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="문산행"
                android:textColor="#000000"
                android:textSize="17sp" />

            <TextView
                android:id="@+id/text_timeFourth"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="00 : 00"
                android:textColor="#FF9900"
                android:textSize="17sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="#7AB097"
            android:backgroundTint="#7AB097"
            android:hapticFeedbackEnabled="false"
            android:orientation="vertical"></LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/text_inputData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="28sp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3" />
</androidx.constraintlayout.widget.ConstraintLayout>